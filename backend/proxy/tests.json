        "messages": [{"role": "user", "content": "Hello"}]
      },
      "expectedStatus": 401,
      "expectedBody": {
        "error": "API key required"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Invalid API key",
      "method": "POST",
      "headers": {
        "X-Api-Key": "invalid_key_12345"
      },
      "body": {
        "model": "gpt-4o-mini",
        "messages": [{"role": "user", "content": "Test"}]
      },
      "expectedStatus": 401,
      "expectedBody": {
        "error": "Invalid API key"
      },
      "bodyMatcher": "partial"
    }
  ]
